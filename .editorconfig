# http://editorconfig.org
root = true

[*.{php,html,twig}]
indent_style = space
indent_size = 4

[*.{js,ts,jsx,tsx,vue,scss,css,sass}]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false
